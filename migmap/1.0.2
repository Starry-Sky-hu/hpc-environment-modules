# for Tcl script use only
set     appname         migmap
set     version         1.0.2
set     prefix          /export/apps/${appname}/${version}
set     url             "https://github.com/mikessh/migmap"
set     msg             "MiGMAP is a mapper for full-length T- and B-cell repertoire sequencing.\nMiGMAP official site: $url\n"

conflict	$appname

module-whatis $msg

prepend-path    PATH                ${prefix}

