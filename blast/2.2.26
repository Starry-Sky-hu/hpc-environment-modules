set version   2.2.26
set appname   blast
set prefix    /export/apps/${appname}/${version}
set exec_prefix   ${prefix}/bin

conflict blast

prepend-path  PATH    ${exec_prefix}
setenv        BLASTDB /export/data/bio/ncbi/blast/db/v4:/export/data/bio/uniprot/blast/db
