set appname         varscan
set version         2.4.2
set prefix          /export/apps/${appname}/${version}
set exec_prefix     ${prefix}

conflict        varscan

prepend-path    PATH    ${exec_prefix}

# varscan requires bam-readcount
if { ![is-loaded bam-readcount] } {
    module load bam-readcount/0.8.0
}

