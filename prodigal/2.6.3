set appname     prodigal
set version     2.6.3
set prefix      /export/apps/${appname}/${version}
set url         "https://github.com/hyattpd/Prodigal"
set msg         "Fast, reliable protein-coding gene prediction for prokaryotic genomes.\Official site: $url\n"

conflict        prodigal

module-whatis   $msg

prepend-path    PATH            ${prefix}/bin

