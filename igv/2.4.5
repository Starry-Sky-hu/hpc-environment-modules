# for Tcl script use only
set     appname         igv
set     version         2.4.5
set     prefix          /export/apps/${appname}/${version}
set     url             "https://software.broadinstitute.org/software/igv"
set     msg             "Integrative Genomics Viewer (IGV).\nOfficial site: $url"

module-whatis $msg

proc ModulesHelp { } {
    puts stderr $msg
}

conflict	$appname

prepend-path    PATH            ${prefix}
