set appname     repseqio
set version     1.3.3
set prefix      /export/apps/${appname}/${version}
set url         "https://github.com/repseqio/repseqio"
set msg         "Utils for manipulations with RepSeq data.\nOfficial site: $url\n"

proc ModulesHelp{ } {
    puts stderr $msg
}

module-whatis   $msg

conflict        $appname

prepend-path    PATH        ${prefix}

